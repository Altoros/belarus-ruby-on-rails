<%= javascript_include_tag "twitter_block" %>
<%= javascript_tag do -%>
  var twitter_blocks_settings = <%= raw twitter_blocks_settings.to_json %>;
<% end -%>
<div class="social">
  <% twitter_blocks_settings.each do |block| %>
    <div class="twtr-search-widget" id="twtr-search-widget-<%= block.id %>"></div>
  <% end %>
</div>
